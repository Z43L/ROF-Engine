{
  "name": "rof-engine",
  "version": "1.0.0",
  "description": "Complete 3D game engine framework for React, React Native and Expo with ECS architecture, physics, post-processing and professional UI system",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "types": "./dist/react/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./systems": {
      "import": "./dist/systems/index.js",
      "require": "./dist/systems/index.cjs",
      "types": "./dist/systems/index.d.ts"
    },
    "./cinematic": {
      "import": "./dist/cinematic/index.js",
      "require": "./dist/cinematic/index.cjs",
      "types": "./dist/cinematic/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build"
  },
  "dependencies": {
    "three": "^0.160.0",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "three": ">=0.160.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "three": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.45",
    "@types/react-native": "^0.72.7",
    "@types/three": "^0.166.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postprocessing": "^6.33.3",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "game-engine",
    "3d",
    "react",
    "react-native",
    "expo",
    "three.js",
    "game-development",
    "ecs",
    "physics",
    "framework"
  ],
  "author": "ReinadoOlvidado Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/reinado-olvidado.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/reinado-olvidado/issues"
  },
  "homepage": "https://github.com/tu-usuario/reinado-olvidado#readme"
}
